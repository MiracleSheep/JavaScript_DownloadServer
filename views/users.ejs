<!DOCTYPE html>
<html lang="en">
<head>
  <title>Users</title>
</head>
<body>
    <div class="table-data">
    <table border="1">
        <tr>
            <th>Name</th>
            <th>Username</th>
            <th>Password</th>
        </tr>
        
        <%
            if(userData.length!=0){
            var i=1;
            userData.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.name %></td>
            <td><%=data.username %></td>
            <td><%=data.password %></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>